<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.msproject.myhome.moara">

    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/moara_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlarmActivity" />
        <activity android:name=".AddCouponActivity" />
        <activity android:name=".ShowCouponActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity
            android:name=".SubmitStoreActivity"
            android:theme="@style/NoTitle01" />
        <activity android:name=".StoreDetailActivity" />
        <activity android:name=".ModifyItem" />
        <activity android:name=".BuyProductDialog" />
        <activity android:name=".SettingAlarmActivity" />
        <activity android:name=".NotificationSomething" />

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>